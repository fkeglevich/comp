declare a: integer;
declare v: integer [10];

main:integer ()
	{
	declare i: integer;
	
	output "Digite um numero: ";
	input a;
	while (i<10)
		{
			{
				a = incn(a,1);
		
				{
					i = i + 1;
					{
						// comando vazio
					};
				
					a = 2	// ultimo comando
				};	
			
				a[i+2] = f(a,1) + 3 + 4	// ultimo comando

			}	// ultimo comando
		};
		
	output "Incrementando algumas vezes fica ",a,"\n"	// ultimo comando
	
	};
	
incn:integer (x:integer, n:integer)
	return x+n;
	
